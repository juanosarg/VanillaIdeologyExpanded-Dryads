		<tickerType>Normal</tickerType>
		<graphicData>
			<graphicClass>Graphic_Single</graphicClass>
			<texPath>Things/Building/UpgradeCarrier</texPath>
		</graphicData>
		<statBases>
			<MaxHitPoints>150</MaxHitPoints>
			<Flammability>1.25</Flammability>
		</statBases>
		<fillPercent>0.5</fillPercent>
		<selectable>true</selectable>
		<comps>
			<li Class="VanillaIdeologyExpanded_Dryads.CompProperties_SpawnAwakened">
				<ticksToSpawn>60000</ticksToSpawn>
				<dryadToSpawn>VDE_AwakenedDryad_Carrier</dryadToSpawn>
			</li>
		</comps>
	</ThingDef>
